[flake8]
ignore = C101,D403,E122,E131,F403,F405,Q000,I100,I201,W503,W504
exclude =
    **/migrations/*,
    .git,
    .mypy_cache,
    __pycache__,
    .venv,
filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity = 12
max-linenumber = 500
max-line-length = 119
multiline-quotes = double
